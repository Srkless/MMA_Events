<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <LinearGradientBrush x:Key="DynamicGradientBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Color="#FF262626" Offset="0"/>
        <GradientStop Color="#FF383838" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="DynamicGradientBackground" EndPoint="0.5,1" StartPoint="0.5,0" Opacity="0.94">
        <GradientStop Color="#FF262626"/>
        <GradientStop Color="#FFF34251" Offset="4"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="buttonColor" Color="#FF262626"/>
    <SolidColorBrush x:Key="color1" Color="#FF980B11"/>
    <SolidColorBrush x:Key="plainTextColor" Color="#FF980B11"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#FF383838"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#FF383838"/>
    <SolidColorBrush x:Key="titleColor3" Color="#FF5F5F5F"/>

    <SolidColorBrush x:Key="reverseColor" Color="#FF980B11"/>
    <Color x:Key="reverseComboBoxColor">#FF980B11</Color>

</ResourceDictionary>
