<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <LinearGradientBrush x:Key="DynamicGradientBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Color="#1e88e5" Offset="0"/>
        <GradientStop Color="#e53935" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="DynamicGradientBackground" EndPoint="0.5,1" StartPoint="0.5,0" Opacity="0.96">
        <GradientStop Color="#2e2b3d"/>
        <GradientStop Color="#d1c4e9" Offset="4"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="buttonColor" Color="#1565c0"/>
    <SolidColorBrush x:Key="color1" Color="#FF262626"/>
    <SolidColorBrush x:Key="plainTextColor" Color="#FF262626"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#607d8b"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="Transparent"/>
    <SolidColorBrush x:Key="titleColor3" Color="#FFFFCDCD"/>
</ResourceDictionary>
