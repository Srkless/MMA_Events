<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <LinearGradientBrush x:Key="DynamicGradientBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Color="#FF980B11" Offset="0"/>
        <GradientStop Color="#FFF34251" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="DynamicGradientBackground" EndPoint="0.5,1" StartPoint="0.5,0" Opacity="0.96">
        <GradientStop Color="#FF262626"/>
        <GradientStop Color="#FF980B11" Offset="4"/>
    </LinearGradientBrush>
    

    <SolidColorBrush x:Key="buttonColor" Color="#FF980B11"/>
    <SolidColorBrush x:Key="color1" Color="#FF262626"/>
    <SolidColorBrush x:Key="plainTextColor" Color="#FF262626"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#FFAF0E14"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#FFEA3444"/>
    <SolidColorBrush x:Key="titleColor3" Color="#FFFFCDCD"/>

    <SolidColorBrush x:Key="reverseColor" Color="#FF980B11"/>
    <Color x:Key="reverseComboBoxColor">#FF980B11</Color>

</ResourceDictionary>
